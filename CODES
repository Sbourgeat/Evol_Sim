python software/reformat_cmh.py --input_cmh 90_percent_selection/1000_SNPs/selection_g500.cmh --input_pvals 90_percent_selection/1000_SNPs/output_g500.txt > 90_percent_selection/1000_SNPs/selection_g500_corrected.cmh



python software/reformat_cmh.py --input_cmh variable_environmentt/selection_g100.cmh --input_pvals variable_environmentt/output_g100.txt > variable_environmentt/selection_g100_corrected.cmh


python software/reformat_cmh.py --input_cmh variable_environmentt/selection_g20.cmh --input_pvals variable_environmentt/output_g20.txt > variable_environmentt/selection_g20_corrected.cmh

